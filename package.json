{
  "name": "Warehouse Inventory Tracker",
  "productName": "warehouse-inv-tracker",
  "version": "1.0.0",
  "description": "Warehouse Inventory Tracker, Tracks incoming/outgoing/current stocked items in warehouses",
  "main": "src/index.js",
  "scripts": {
    "install-all": "npm install && install-routify",
    "install-routify": "cd src/client && npm install",
    "dev": "cd src/client && npm run dev",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [
    "Electronjs"
  ],
  "author": {
    "name": "NZ",
    "email": "v.adam.gonzales@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "concurrently": "^7.0.0",
    "electron": "17.1.0"
  }
}